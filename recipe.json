[
    {
        "name": "Honey Glazed Ham",
        "description": "This ham tastes very much like the famous honey baked ham but costs much less, and there's no need to fight the crowds at holiday time. You can even buy the ham presliced to make it easier and more like the original. It is very good. (I do this while preparing the rest of the meal in the kitchen so that I don't forget to baste!)",
        "ingredients": [
            {
                "quantity": {
                    "value": "5",
                    "uom": "lbs"
                },
                "name": "Ham"
            },
            {
                "quantity": {
                    "value": "1/4",
                    "uom": "cup"
                },
                "name": "Cloves"
            },
            {
                "quantity": {
                    "value": "1/4",
                    "uom": "cup"
                },
                "name": "Dark Corn Syrup"
            },
            {
                "quantity": {
                    "value": "2",
                    "uom": "cups"
                },
                "name": "Honey"
            },
            {
                "quantity": {
                    "value": "2/3",
                    "uom": "cup"
                },
                "name": "Butter"
            }
        ], 
        "directions": [
            {
                "step": 1,
                "direction": "Preheat oven to 325 degrees F (165 degrees C)."
            },
            {
                "step": 2,
                "direction": "Score ham, and stud with the whole cloves. Place ham in foil lined pan."
            },
            {
                "step": 3,
                "direction": "In the top half of a double boiler, heat the corn syrup, honey and butter. Keep glaze warm while baking ham."
            },
            {
                "step": 4,
                "direction": "Brush glaze over ham, and bake for 1 hour and 15 minutes in the preheated oven. Baste ham every 10 to 15 minutes with the honey glaze. During the last 4 to 5 minutes of baking, turn on broiler to caramelize the glaze. Remove from oven, and let sit a few minutes before serving."
            }
        ],
        "yield": 15,
        "image": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fimages.media-allrecipes.com%2Fuserphotos%2F630590.jpg"
    },
